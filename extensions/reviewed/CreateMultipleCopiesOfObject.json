            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Object",
                "ColumnID",
                "=",
                "Variable(__CreateMultipleCopiesOfObject.x)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Object",
                "CreationID",
                "=",
                "Variable(__CreateMultipleCopiesOfObject.CreationID)"
