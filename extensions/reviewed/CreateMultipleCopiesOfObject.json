    {
      "description": "Create a ring of objects around a center object. The size and shape of the ring are based on the dimensions of the center object.",
      "fullName": "Create a ring of objects around a center object",
      "functionType": "Action",
      "name": "CreateRingOfObjectsAroundObject",
      "sentence": "Create a ring of _PARAM2_ instances of _PARAM1_ around _PARAM3_ (Rotate towards center: _PARAM4_, Angle offset: _PARAM5_ degrees)",
      "events": [
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [
            {
              "type": {
