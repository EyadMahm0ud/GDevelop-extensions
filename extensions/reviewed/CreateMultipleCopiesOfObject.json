    {
      "description": "Create a ring of objects around a position.",
      "fullName": "Create a ring of objects",
      "functionType": "Action",
      "name": "CreateRingOfObjects",
      "sentence": "Create a ring of _PARAM2_ instances of _PARAM1_ centered at _PARAM4_;_PARAM5_ at a radius of _PARAM3_ px (Rotate towards center: _PARAM6_, Angle offset: _PARAM7_)",
      "events": [
        {
          "colorB": 228,
          "colorG": 176,
          "colorR": 74,
          "creationTime": 0,
          "name": "Temp variables to track progress",
          "source": "",
          "type": "BuiltinCommonInstructions::Group",
          "events": [
            {
              "type": "BuiltinCommonInstructions::Standard",
              "conditions": [],
              "actions": [
                {
                  "type": {
                    "value": "ModVarScene"
                  },
                  "parameters": [
                    "__CreateMultipleCopiesOfObject.CreationID",
                    "=",
                    "1"
                  ]
                }
              ]
            }
          ],
          "parameters": []
        },
        {
          "colorB": 228,
          "colorG": 176,
          "colorR": 74,
          "creationTime": 0,
          "name": "Create objects",
          "source": "",
          "type": "BuiltinCommonInstructions::Group",
          "events": [
            {
              "infiniteLoopWarning": true,
              "type": "BuiltinCommonInstructions::While",
              "whileConditions": [
                {
                  "type": {
                    "value": "VarScene"
                  },
                  "parameters": [
                    "__CreateMultipleCopiesOfObject.CreationID",
                    "<=",
                    "GetArgumentAsNumber(\"ObjectQuantity\")"
                  ]
                }
              ],
              "conditions": [],
              "actions": [
                {
                  "type": {
                    "value": "Create"
                  },
                  "parameters": [
                    "",
                    "Object",
                    "GetArgumentAsNumber(\"CenterPositionX\")",
                    "GetArgumentAsNumber(\"CenterPositionY\")",
                    "GetArgumentAsString(\"Layer\")"
                  ]
                },
                {
                  "type": {
                    "value": "ModVarObjet"
                  },
                  "parameters": [
                    "Object",
                    "CreationID",
                    "=",
                    "Variable(__CreateMultipleCopiesOfObject.CreationID)"
                  ]
                },
                {
                  "type": {
                    "value": "MettreAutourPos"
                  },
                  "parameters": [
                    "Object",
                    "GetArgumentAsNumber(\"CenterPositionX\")",
                    "GetArgumentAsNumber(\"CenterPositionY\")",
                    "GetArgumentAsNumber(\"Radius\")",
                    "360 * ((Variable(__CreateMultipleCopiesOfObject.CreationID) - 1) / GetArgumentAsNumber(\"ObjectQuantity\"))"
                  ]
                }
              ],
              "events": [
                {
                  "type": "BuiltinCommonInstructions::Standard",
                  "conditions": [
                    {
                      "type": {
                        "value": "GetArgumentAsBoolean"
                      },
                      "parameters": [
                        "\"RotateTowardsCenter\""
                      ]
                    }
                  ],
                  "actions": [
                    {
                      "type": {
                        "value": "SetAngle"
                      },
                      "parameters": [
                        "Object",
                        "=",
                        "Object.AngleToPosition(GetArgumentAsNumber(\"CenterPositionX\"), GetArgumentAsNumber(\"CenterPositionY\")) + GetArgumentAsNumber(\"AngleOffset\")"
                      ]
                    }
                  ]
                },
                {
                  "type": "BuiltinCommonInstructions::Standard",
                  "conditions": [],
                  "actions": [
                    {
                      "type": {
                        "value": "ModVarScene"
                      },
                      "parameters": [
                        "__CreateMultipleCopiesOfObject.CreationID",
                        "+",
                        "1"
                      ]
                    }
                  ]
                }
              ]
            }
          ],
          "parameters": []
        }
      ],
