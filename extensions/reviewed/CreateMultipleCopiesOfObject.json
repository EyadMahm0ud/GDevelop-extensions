                  "type": {
                    "value": "ModVarScene"
                  },
                  "parameters": [
                    "__CreateMultipleCopiesOfObject.x",
                    "=",
                    "1"
                  ]
                },
                {
                  "type": {
                    "value": "ModVarScene"
                  },
                  "parameters": [
                    "__CreateMultipleCopiesOfObject.y",
                    "+",
                    "1"
                  ]
                }
              ],
              "events": [
                {
                  "type": "BuiltinCommonInstructions::Comment",
                  "color": {
                    "b": 109,
                    "g": 230,
                    "r": 255,
                    "textB": 0,
                    "textG": 0,
                    "textR": 0
                  },
                  "comment": "If the last row is complete, stop creating new objects."
