            },
            {
              "type": {
                "value": "ModVarObjetTxt"
              },
              "parameters": [
                "Object",
                "BatchID",
                "=",
                "GetArgumentAsString(\"BatchID\")"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "__CreateMultipleCopiesOfObject.x",
                "+",
                "1"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "__CreateMultipleCopiesOfObject.CreationID",
                "+",
                "1"
              ]
            }
          ],
          "events": [
            {
              "type": "BuiltinCommonInstructions::Standard",
              "conditions": [
                {
                  "type": {
                    "value": "Egal"
                  },
                  "parameters": [
                    "GetArgumentAsNumber(\"Zorder\")",
