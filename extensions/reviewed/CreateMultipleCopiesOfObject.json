                  "type": "BuiltinCommonInstructions::Standard",
                  "conditions": [
                    {
                      "type": {
                        "value": "VarScene"
                      },
                      "parameters": [
                        "__CreateMultipleCopiesOfObject.y",
                        ">",
                        "GetArgumentAsNumber(\"NumberOfRows\")"
