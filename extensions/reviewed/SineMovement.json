                  "type": {
                    "value": "MettreY"
                  },
                  "parameters": [
                    "Object",
                    "=",
                    "Object.Behavior::PropertyCenterPointY() + sin(ToRad(Object.Behavior::PropertySineProgressY())) * Object.Behavior::PropertyVerticalDistance()"
                  ]
                }
              ]
            },
