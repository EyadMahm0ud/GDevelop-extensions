      "description": "Create multiple objects.",
      "fullName": "Create multiple objects",
      "functionType": "Action",
      "name": "CreateMultipleObject",
      "sentence": "Create multiple _PARAM1_ objects",
      "events": [
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [],
          "actions": [
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "__CreateMultipleObject.Row",
                "=",
                "0"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "__CreateMultipleObject.Rows",
                "=",
                "floor(sqrt(GetArgumentAsNumber(\"Amount\")))"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "__CreateMultipleObject.Column",
                "=",
                "0"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "__CreateMultipleObject.Columns",
                "=",
                "round(GetArgumentAsNumber(\"Amount\") / Variable(__CreateMultipleObject.Rows))"
              ]
            }
          ]
        },
        {
          "infiniteLoopWarning": true,
          "type": "BuiltinCommonInstructions::While",
          "whileConditions": [
            {
              "type": {
                "value": "VarScene"
              },
              "parameters": [
                "__CreateMultipleObject.Row",
                "<",
                "Variable(__CreateMultipleObject.Rows)"
              ]
            }
          ],
          "conditions": [],
          "actions": [],
          "events": [
            {
              "infiniteLoopWarning": true,
              "type": "BuiltinCommonInstructions::While",
              "whileConditions": [
                {
                  "type": {
                    "value": "VarScene"
                  },
                  "parameters": [
                    "__CreateMultipleObject.Column",
                    "<",
                    "Variable(__CreateMultipleObject.Columns)"
                  ]
                }
              ],
              "conditions": [],
              "actions": [
                {
                  "type": {
                    "value": "Create"
                  },
                  "parameters": [
                    "",
                    "Object",
                    "0",
                    "0",
                    "GetArgumentAsString(\"Layer\")"
                  ]
                },
                {
                  "type": {
                    "value": "SetAnimationName"
                  },
                  "parameters": [
                    "Object",
                    "GetArgumentAsString(\"AnimationName\")"
                  ]
                },
                {
                  "type": {
                    "value": "ChangePlan"
                  },
                  "parameters": [
                    "Object",
                    "=",
                    "GetArgumentAsNumber(\"ZOrder\")"
                  ]
                }
              ],
              "events": [
                {
                  "type": "BuiltinCommonInstructions::Standard",
                  "conditions": [
                    {
                      "type": {
                        "value": "CompareArgumentAsString"
                      },
                      "parameters": [
                        "\"Position\"",
                        "=",
                        "\"top-left\""
                      ]
                    }
                  ],
                  "actions": [
                    {
                      "type": {
                        "value": "MettreX"
                      },
                      "parameters": [
                        "Object",
                        "=",
                        "GetArgumentAsNumber(\"X\") + (Variable(__CreateMultipleObject.Column) * Object.Width())"
                      ]
                    },
                    {
                      "type": {
                        "value": "MettreY"
                      },
                      "parameters": [
                        "Object",
                        "=",
                        "GetArgumentAsNumber(\"Y\") - (Variable(__CreateMultipleObject.Row) * Object.Height())"
                      ]
                    }
                  ]
                },
                {
                  "type": "BuiltinCommonInstructions::Standard",
                  "conditions": [
                    {
                      "type": {
                        "value": "CompareArgumentAsString"
                      },
