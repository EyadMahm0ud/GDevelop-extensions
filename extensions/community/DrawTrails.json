{
  "author": "",
  "category": "Visual effect",
  "extensionNamespace": "",
  "fullName": "Draw trails",
  "helpPath": "",
  "iconUrl": "data:image/svg+xml;base64,",
  "name": "DrawTrails",
  "previewIconUrl": "https://asset-resources.gdevelop.io/public-resources/Icons/ecaf93beba43dbf689ab7e793b439b4f56565c9ba7023983627f0231ed799024_motion-outline.svg",
  "shortDescription": "A trail extension to draw trails that follow a certain position.",
  "version": "1.0.0",
  "description": [
    "This extension comes with 2 types of trail creation, each one with 2 actions, the trail initialization action and the trail creation action, the first one should always run before the second one to get better results.",
    "",
    "Particle trails require too much power while the shape painted ones don't do that much."
  ],
  "tags": [
    "Visual effect"
  ],
  "authorIds": [
    "SEnxPnvGuQhQmTXJ3xwZa1lQFdr1"
  ],
  "dependencies": [],
  "eventsFunctions": [
    {
      "description": "Declares a trail, use the end point action to create it.",
      "fullName": "Initial point",
      "functionType": "Action",
      "group": "Particle trail",
      "name": "ParticleTrailStartPoint",
      "sentence": "Initialize particle trail _PARAM1_ on x: _PARAM2_ y: _PARAM3_",
      "events": [
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [],
          "actions": [
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].OldX",
                "=",
                "GetArgumentAsNumber(\"X\")"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].OldY",
                "=",
                "GetArgumentAsNumber(\"Y\")"
              ]
            }
          ]
        }
      ],
      "parameters": [
        {
          "description": "The name of the trail",
          "name": "TrailName",
          "type": "string"
        },
        {
          "description": "The start X position of the trail",
          "name": "X",
          "type": "expression"
        },
        {
          "description": "The start Y position of the trail",
          "name": "Y",
          "type": "expression"
        }
      ],
      "objectGroups": []
    },
    {
      "description": "Creates the trail from the start to the end.",
      "fullName": "End point",
      "functionType": "Action",
      "group": "Particle trail",
      "name": "ParticleTrailEndPoint",
      "sentence": "Particle trail _PARAM1_ ends in x: _PARAM3_ and y: _PARAM4_ with _PARAM5_ pixels per _PARAM2_ on tank: _PARAM8_ z: _PARAM6_ layer: _PARAM7_",
      "events": [
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [],
          "actions": [
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].Cdistance",
                "=",
                "DistanceBetweenPositions(Variable(TrailExt[GetArgumentAsString(\"TrailName\")].OldX), Variable(TrailExt[GetArgumentAsString(\"TrailName\")].OldY), GetArgumentAsNumber(\"X\"), GetArgumentAsNumber(\"Y\"))"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].Angle",
                "=",
                "AngleBetweenPositions(Variable(TrailExt[GetArgumentAsString(\"TrailName\")].OldX), Variable(TrailExt[GetArgumentAsString(\"TrailName\")].OldY), GetArgumentAsNumber(\"X\"), GetArgumentAsNumber(\"Y\"))"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].DPP",
                "=",
                "max(1, GetArgumentAsNumber(\"DistancePerObject\"))"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].Z",
                "=",
                "GetArgumentAsNumber(\"Z\")"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].Layer",
                "=",
                "GetArgumentAsNumber(\"Layer\")"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].Capacity",
                "=",
                "GetArgumentAsNumber(\"Capacity\")"
              ]
            }
          ]
        },
        {
          "infiniteLoopWarning": true,
          "type": "BuiltinCommonInstructions::While",
          "whileConditions": [
            {
              "type": {
                "value": "VarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].Cdistance",
                ">",
                "0"
              ]
            }
          ],
          "conditions": [],
          "actions": [
            {
              "type": {
                "value": "Create"
              },
              "parameters": [
                "",
                "Particle",
                "GetArgumentAsNumber(\"X\")",
                "GetArgumentAsNumber(\"Y\")",
                "VariableString(TrailExt[GetArgumentAsString(\"TrailName\")].Layer)"
              ]
            },
            {
              "type": {
                "value": "ParticleSystem::SetTank"
              },
              "parameters": [
                "Particle",
                "=",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].Capacity)"
              ]
            },
            {
              "type": {
                "value": "ChangePlan"
              },
              "parameters": [
                "Particle",
                "=",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].Z)"
              ]
            },
            {
              "type": {
                "value": "MettreAutourPos"
              },
              "parameters": [
                "Particle",
                "GetArgumentAsNumber(\"X\")",
                "GetArgumentAsNumber(\"Y\")",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].Cdistance)",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].Angle)"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].Cdistance",
                "-",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].DPP)"
              ]
            }
          ]
        },
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [],
          "actions": [
            {
              "type": {
                "value": "Create"
              },
              "parameters": [
                "",
                "Particle",
                "GetArgumentAsNumber(\"X\")",
                "GetArgumentAsNumber(\"Y\")",
                "VariableString(TrailExt[GetArgumentAsString(\"TrailName\")].Layer)"
              ]
            },
            {
              "type": {
                "value": "ChangePlan"
              },
              "parameters": [
                "Particle",
                "=",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].Z)"
              ]
            },
            {
              "type": {
                "value": "ParticleSystem::SetTank"
              },
              "parameters": [
                "Particle",
                "=",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].Capacity)"
              ]
            }
          ]
        }
      ],
      "parameters": [
        {
          "description": "The name of the trail",
          "name": "TrailName",
          "type": "string"
        },
        {
          "description": "The particle object that creates the trail",
          "name": "Particle",
          "supplementaryInformation": "ParticleSystem::ParticleEmitter",
          "type": "objectList"
        },
        {
          "description": "The end X position of the trail",
          "name": "X",
          "type": "expression"
        },
        {
          "description": "The end Y position of the trail",
          "name": "Y",
          "type": "expression"
        },
        {
          "description": "The pixels per particle on the trail (1 = highest quality, lowest performance)  ",
          "name": "DistancePerObject",
          "type": "expression"
        },
        {
          "description": "The z order of the trail",
          "name": "Z",
          "type": "expression"
        },
        {
          "description": "The layer of the trail",
          "name": "Layer",
          "type": "layer"
        },
        {
          "description": "The particle tank of the trail",
          "name": "Capacity",
          "type": "expression"
        }
      ],
      "objectGroups": []
    },
    {
      "description": "Declares a trail, use the end point action to create it.",
      "fullName": "Initial point",
      "functionType": "Action",
      "group": "Paint trail",
      "name": "ShapeTrailStartPoint",
      "sentence": "Initialize paint trail _PARAM1_ on x: _PARAM2_ y: _PARAM3_",
      "events": [
        {
