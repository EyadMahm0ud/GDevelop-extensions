    {
      "description": "Creates the trail from the start to the end.",
      "fullName": "End point",
      "functionType": "Action",
      "group": "Paint trail",
      "name": "ShapeTrailEndPoint",
      "sentence": "Paint trail _PARAM1_ of _PARAM2_ color: _PARAM3_ ends in x: _PARAM4_ y: _PARAM5_ z: _PARAM6_ layer: _PARAM7_",
      "events": [
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [],
          "actions": [
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].PaintLength",
                "=",
                "GetArgumentAsNumber(\"LifeLength\")"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].PaintSize",
                "=",
                "GetArgumentAsNumber(\"Size\")"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].PaintSizeEnd",
                "=",
                "GetArgumentAsNumber(\"SizeEnd\")"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].PaintZ",
                "=",
                "GetArgumentAsNumber(\"Z\")"
              ]
            },
            {
              "type": {
                "value": "ModVarSceneTxt"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].PaintLayer",
                "=",
                "GetArgumentAsString(\"Layer\")"
              ]
            },
            {
              "type": {
                "value": "ModVarSceneTxt"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].PaintColor",
                "=",
                "GetArgumentAsString(\"Color\")"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].PaintO",
                "=",
                "GetArgumentAsNumber(\"Opacity\")"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].PaintOend",
                "=",
                "GetArgumentAsNumber(\"OpacityEnd\")"
              ]
            }
          ]
        },
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [
            {
              "type": {
                "value": "BuiltinCommonInstructions::Once"
              },
              "parameters": []
            }
          ],
          "actions": [
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "TrailExt[GetArgumentAsString(\"TrailName\")].TimesUsed",
                "+",
                "1"
              ]
            }
          ]
        },
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [
            {
              "type": {
                "value": "VarObjetTxt"
              },
              "parameters": [
                "Painter",
                "TrailExt.IsRunning",
                "!=",
                "ToString(Variable(TrailExt[GetArgumentAsString(\"TrailName\")].TimesUsed))"
              ]
            }
          ],
          "actions": [
            {
              "type": {
                "value": "Delete"
              },
              "parameters": [
                "Painter",
                ""
              ]
            }
          ]
        },
        {
          "folded": true,
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [
            {
              "type": {
                "value": "VarObjetTxt"
              },
              "parameters": [
                "Painter",
                "TrailExt.ID",
                "=",
                "GetArgumentAsString(\"TrailName\")"
              ]
            }
          ],
          "actions": [
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.Life",
                "-",
                "2"
              ]
            }
          ],
          "events": [
            {
              "type": "BuiltinCommonInstructions::Standard",
              "conditions": [
                {
                  "type": {
                    "value": "VarObjet"
                  },
                  "parameters": [
                    "Painter",
                    "TrailExt.Life",
                    "<=",
                    "0"
                  ]
                }
              ],
              "actions": [
                {
                  "type": {
                    "value": "Delete"
                  },
                  "parameters": [
                    "Painter",
                    ""
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [],
          "actions": [
            {
              "type": {
                "value": "Create"
              },
              "parameters": [
                "",
                "Painter",
                "0",
                "0",
                "VariableString(TrailExt[GetArgumentAsString(\"TrailName\")].PaintLayer)"
              ]
            },
            {
              "type": {
                "value": "PrimitiveDrawing::ClearBetweenFrames"
              },
              "parameters": [
                "Painter",
                "yes"
              ]
            },
            {
              "type": {
                "value": "ChangePlan"
              },
              "parameters": [
                "Painter",
                "=",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].PaintZ)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjetTxt"
              },
              "parameters": [
                "Painter",
                "TrailExt.ID",
                "=",
                "GetArgumentAsString(\"TrailName\")"
              ]
            },
            {
              "type": {
                "value": "ModVarObjetTxt"
              },
              "parameters": [
                "Painter",
                "TrailExt.Color",
                "=",
                "VariableString(TrailExt[GetArgumentAsString(\"TrailName\")].PaintColor)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.IsRunning",
                "=",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].TimesUsed)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.Size",
                "=",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].PaintSizeEnd)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.SizeEnd",
                "=",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].PaintSize)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.Oend",
                "=",
                "min(255, abs(Variable(TrailExt[GetArgumentAsString(\"TrailName\")].PaintO)))"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.O",
                "=",
                "min(255, abs(Variable(TrailExt[GetArgumentAsString(\"TrailName\")].PaintOend)))"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.Life",
                "=",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].PaintLength)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.MaxLife",
                "=",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].PaintLength)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.SX",
                "=",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].PaintOldX)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.SY",
                "=",
                "Variable(TrailExt[GetArgumentAsString(\"TrailName\")].PaintOldY)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.X",
                "=",
                "GetArgumentAsNumber(\"X\")"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.Y",
                "=",
                "GetArgumentAsNumber(\"Y\")"
              ]
            }
          ]
        },
        {
          "type": "BuiltinCommonInstructions::ForEach",
          "object": "Painter",
          "conditions": [
            {
              "type": {
                "value": "VarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.Life",
                ">",
                "0"
              ]
            },
            {
              "type": {
                "value": "VarObjetTxt"
              },
              "parameters": [
                "Painter",
                "TrailExt.ID",
                "=",
                "GetArgumentAsString(\"TrailName\")"
              ]
            }
          ],
          "actions": [
            {
              "type": {
                "value": "PrimitiveDrawing::OutlineColor"
              },
              "parameters": [
                "Painter",
                "Painter.VariableString(TrailExt.Color)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.SizeRLife",
                "=",
                "Painter.Variable(TrailExt.Life)/Painter.Variable(TrailExt.MaxLife)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.SizeDiff",
                "=",
                "Painter.Variable(TrailExt.Size) - Painter.Variable(TrailExt.SizeEnd)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.SizeR",
                "=",
                "Painter.Variable(TrailExt.Size) - (Painter.Variable(TrailExt.SizeDiff) * Painter.Variable(TrailExt.SizeRLife))"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.Odiff",
                "=",
                "Painter.Variable(TrailExt.O) - Painter.Variable(TrailExt.Oend)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "Painter",
                "TrailExt.OR",
                "=",
                "Painter.Variable(TrailExt.O) - (Painter.Variable(TrailExt.Odiff) * Painter.Variable(TrailExt.SizeRLife))"
              ]
            },
            {
              "type": {
                "value": "PrimitiveDrawing::FillOpacity"
              },
              "parameters": [
                "Painter",
                "=",
                "Painter.Variable(TrailExt.OR)"
              ]
            },
            {
              "type": {
                "value": "PrimitiveDrawing::OutlineOpacity"
              },
              "parameters": [
                "Painter",
                "=",
                "Painter.Variable(TrailExt.OR)"
              ]
            },
            {
              "type": {
                "value": "PrimitiveDrawing::OutlineSize"
              },
              "parameters": [
                "Painter",
                "=",
                "50 * Painter.Variable(TrailExt.SizeR)"
              ]
            },
            {
              "type": {
                "value": "PrimitiveDrawing::BezierCurve"
              },
              "parameters": [
                "Painter",
                "Painter.Variable(TrailExt.SX)",
                "Painter.Variable(TrailExt.SY)",
                "Painter.Variable(TrailExt.SX)",
                "Painter.Variable(TrailExt.SY)",
                "Painter.Variable(TrailExt.X)",
                "Painter.Variable(TrailExt.Y)",
                "Painter.Variable(TrailExt.X)",
                "Painter.Variable(TrailExt.Y)"
              ]
            }
          ]
        }
      ],
      "parameters": [
        {
          "description": "The name of the trail",
          "name": "TrailName",
          "supplementaryInformation": "objectTrail",
          "type": "string"
        },
        {
          "description": "The particle object that creates the trail",
          "name": "Painter",
          "supplementaryInformation": "PrimitiveDrawing::Drawer",
          "type": "objectList"
        },
        {
          "description": "The color of the trail",
          "name": "Color",
          "type": "color"
        },
        {
          "description": "The end X position of the trail",
          "name": "X",
          "type": "expression"
        },
        {
          "description": "The end Y position of the trail",
          "name": "Y",
          "type": "expression"
        },
        {
          "description": "The z order of the trail",
          "name": "Z",
          "type": "expression"
        },
        {
          "description": "The layer of the trail",
          "name": "Layer",
          "type": "layer"
        },
        {
          "description": "The length of the trail",
          "name": "LifeLength",
          "type": "expression"
        },
        {
          "description": "The start size of the trail",
          "name": "Size",
          "type": "expression"
        },
        {
          "description": "The end size of the trail",
          "name": "SizeEnd",
          "type": "expression"
        },
        {
          "description": "The start opacity of the trail",
          "name": "Opacity",
          "type": "expression"
        },
        {
          "description": "The end opacity of the trail",
          "name": "OpacityEnd",
          "type": "expression"
        }
      ],
      "objectGroups": []
    }
  ],
