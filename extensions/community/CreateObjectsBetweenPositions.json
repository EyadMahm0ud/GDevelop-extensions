  "eventsFunctions": [
    {
      "description": "Create a chain of objects between two positions.",
      "fullName": "Create Objects Between Positions",
      "functionType": "Action",
      "group": "",
      "name": "CreateObjectsBetweenPositions",
      "private": false,
      "sentence": "Create object _PARAM1_ between positions (_PARAM2_,_PARAM3_) and (_PARAM4_,_PARAM5_), _PARAM6_ objects are created",
      "events": [
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [],
          "actions": [
            {
              "type": {
                "value": "ModVarGlobal"
              },
              "parameters": [
                "ObjectsBetweenPositions.XChange",
                "=",
                "(GetArgumentAsNumber(\"End_Position_X\")-GetArgumentAsNumber(\"Start_Position_X\"))/GetArgumentAsNumber(\"TotalNumber\")"
              ]
            },
            {
              "type": {
                "value": "ModVarGlobal"
              },
              "parameters": [
                "ObjectsBetweenPositions.YChange",
                "=",
                "(GetArgumentAsNumber(\"End_Position_Y\")-GetArgumentAsNumber(\"Start_Position_Y\"))/GetArgumentAsNumber(\"TotalNumber\")"
              ]
            },
            {
              "type": {
                "value": "ModVarGlobal"
              },
              "parameters": [
                "ObjectsBetweenPositions.TotalCount",
                "=",
                "0"
              ]
            }
          ],
          "events": [
            {
              "infiniteLoopWarning": true,
              "type": "BuiltinCommonInstructions::While",
              "whileConditions": [
                {
                  "type": {
                    "value": "VarGlobal"
                  },
                  "parameters": [
                    "ObjectsBetweenPositions.TotalCount",
                    "<=",
                    "GetArgumentAsNumber(\"TotalNumber\")"
                  ]
                }
              ],
              "conditions": [],
              "actions": [
                {
                  "type": {
                    "value": "Create"
                  },
                  "parameters": [
                    "",
                    "Object",
                    "GetArgumentAsNumber(\"Start_Position_X\") + (GlobalVariable(ObjectsBetweenPositions.XChange)*GlobalVariable(ObjectsBetweenPositions.TotalCount))",
                    "GetArgumentAsNumber(\"Start_Position_Y\") + (GlobalVariable(ObjectsBetweenPositions.YChange)*GlobalVariable(ObjectsBetweenPositions.TotalCount))",
                    "\"\""
                  ]
                },
                {
                  "type": {
                    "value": "ModVarGlobal"
                  },
                  "parameters": [
                    "ObjectsBetweenPositions.TotalCount",
                    "+",
                    "1"
                  ]
                }
              ]
            }
          ]
        }
      ],
      "parameters": [
        {
          "codeOnly": false,
          "defaultValue": "",
          "description": "Object To Create",
          "longDescription": "",
          "name": "Object",
          "optional": false,
